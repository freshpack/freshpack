  },
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "bin": {
    "freshpack": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://git@github.com:freshpack/freshpack.git"
  },
  "bugs": {
    "url": "https://github.com/freshpack/freshpack/issues"
  },
  "homepage": "http://freshpack.rocks/",
  "keywords": [
    "scaffolding",
