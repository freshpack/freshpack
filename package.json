    "sass",
    "eslint",
    "test",
    "webpack",
    "babel",
    "hot-reloading"
  ],
  "dependencies": {
    "colors": "^1.1.2",
    "command-line-args": "^4.0.2",
    "elegant-spinner": "^1.0.1",
    "log-update": "^1.0.2",
    "object.entries": "^1.0.4",
    "prompt": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3"
