{
  "name": "freshpack",
  "version": "0.1.9",
  "description": "Command line scaffolding tool for react apps",
  "author": "Uli Preuss",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "node index.js --sass --lint --test",
    "lint": "eslint index.js ./src -f table || true",
